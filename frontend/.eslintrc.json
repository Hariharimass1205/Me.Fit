{
  "extends": [
    "next/core-web-vitals",
    "next/typescript"
  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/no-unused-vars": "off",
    "@typescript-eslint/no-wrapper-object-types": "off",
    "prefer-const": "off",
    "react-hooks/exhaustive-deps": "off", // Disable the warning for missing dependencies in useEffect
    "react-hooks/rules-of-hooks": "off"  // Optionally, disable the hook rules if needed
  }
}